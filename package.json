{
  "name": "najwer23snacks",
  "version": "0.0.8",
  "main": "./lib/index.js",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.ts",
  "author": "Mariusz Najwer (https://najwer23.github.io/)",
  "homepage": "https://najwer23.github.io/najwer23snacks",
  "type": "module",
  "scripts": {
    "deploy": "yarn run build-storybook && gh-pages -d storybook-static && rimraf ./storybook-static ",
    "start": "storybook dev -p 6006",
    "build": "rimraf ./lib && rollup -c --silent",
    "build-storybook": "storybook build",
    "pub": "npm version patch --force && npm publish",
    "pub-minor": "npm version minor --force && npm publish",
    "pub-major": "npm version major --force && npm publish",
    "god": "yarn build && yarn pub && rimraf ./lib && rimraf ./.yarn/cache",
    "plop": "plop && yarn format",
    "format": "prettier --write \"**/*.{json,mdx,ts,tsx,md,css,mjs}\""
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.8.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "0.4.4",
    "@storybook/addon-actions": "^8.2.9",
    "@storybook/addon-console": "^3.0.0",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-interactions": "^8.2.9",
    "@storybook/addon-links": "^8.2.9",
    "@storybook/addon-onboarding": "^8.2.9",
    "@storybook/blocks": "^8.2.9",
    "@storybook/react": "^8.2.9",
    "@storybook/react-vite": "^8.2.9",
    "@storybook/test": "^8.2.9",
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "gh-pages": "^6.1.1",
    "plop": "^4.0.1",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^6.0.1",
    "rollup": "^4.21.1",
    "rollup-plugin-dts": "6.1.1",
    "rollup-plugin-output-size": "^1.4.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "0.36.0",
    "storybook": "^8.2.9",
    "storybook-css-modules-preset": "^1.1.1",
    "typescript": "^5.5.4",
    "vite": "^5.4.2"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
